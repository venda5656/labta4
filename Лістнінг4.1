# Константа розміру таблиці (M = 13)
M = 13
# Вхідні слова (окремі елементи хешування)
WORDS = [
    "СЛОВО",
    "НЕ1",
    "ГОРОБЕЦЬ",
    "ВИЛЕТИТЬ",
    "НЕ2",
    "СПІЙМАЄШ"
]
# Словник позицій українського алфавіту (1–33)
LETTER_POSITIONS = {
    'А': 1,  'Б': 2,  'В': 3,  'Г': 4,  'Ґ': 5,  'Д': 6,
    'Е': 7,  'Є': 8,  'Ж': 9,  'З': 10, 'И': 11, 'І': 12,
    'Ї': 13, 'Й': 14, 'К': 15, 'Л': 16, 'М': 17, 'Н': 18,
    'О': 19, 'П': 20, 'Р': 21, 'С': 22, 'Т': 23, 'У': 24,
    'Ф': 25, 'Х': 26, 'Ц': 27, 'Ч': 28, 'Ш': 29, 'Щ': 30,
    'Ь': 31, 'Ю': 32, 'Я': 33
}
def simple_hash_from_map(key: str) -> int:
    """
    Хеш-функція: h(k) = (сума позицій букв key) mod 13.
    Нерозпізнані символи дають 0.
    """
    sum_of_positions = 0
    for char in key:
        pos = LETTER_POSITIONS.get(char, 0)
        sum_of_positions += pos
    hash_address = sum_of_positions % M
    return hash_address
def build_open_hash_table(words: list, m: int) -> list:
    """
    Побудова відкритої хеш-таблиці (ланцюжки).
    """
    # 1. Створюємо M порожніх списків
    hash_table = [[] for _ in range(m)]
    # 2. Додаємо слова за їх хеш-адресою
    for word in words:
        address = simple_hash_from_map(word)
        hash_table[address].append(word)
 return hash_table
def display_hash_table(table: list):
    """Вивід хеш-таблиці в зручному форматі."""
    print("\n--- Хеш-таблиця (метод ланцюжків, M = 13) ---")
    for i, chain in enumerate(table):
        print(f"Індекс {i:02d}: {chain}")
# Побудова й виведення відкритої хеш-таблиці
hash_table = build_open_hash_table(WORDS, M)
display_hash_table(hash_table)
